msgstr "Nieuw JavaScript-beleid"

#: jsopts.cpp:215
#, kde-format
msgctxt "@title:window"
msgid "Change JavaScript Policy"
msgstr "JavaScript-beleid wijzigen"

#: jsopts.cpp:219
#, kde-format
msgid "JavaScript policy:"
msgstr "JavaScript-beleid:"

#: jsopts.cpp:220
#, kde-format
